{
  "name": "gad_tests",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "check": "npm run format && npm run lint && npm run tsc:check",
    "format": "npx prettier --write .",
    "format:check": "npx prettier . --check \"!**.ts\"",
    "lint": "npx eslint . --max-warnings=0",
    "prepare": "husky",
    "test": "npx playwright test",
    "test:headed": "npm run test -- --headed",
    "test:ui-mode": "npm run test -- --ui",
    "show-report": "npx playwright show-report",
    "tsc:check": "npx tsc --noEmit --pretty --strict",
    "test:tag": "npx playwright test --grep \"@GAD-R01-02|@GAD-R01-01\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@playwright/test": "^1.52.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^22.10.10",
    "dotenv": "^16.4.7",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-playwright": "^2.2.0",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "prettier": "3.4.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.22.0"
  },
  "dependencies": {
    "@faker-js/faker": "^9.5.0"
  }
}
